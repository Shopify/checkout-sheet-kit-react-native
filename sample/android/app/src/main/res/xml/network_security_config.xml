<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <!-- Trust user installed certificates -->
            <certificates src="user"/>
            <!-- Trust system CAs -->
            <certificates src="system"/>
        </trust-anchors>
    </base-config>
</network-security-config>
