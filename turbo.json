{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {},
    "build": {},
    "build:android": {
      "cache": false,
      "inputs": [
        "sample/package.json",
        "sample/android",
        "sample/src/**/*",
        "!sample/android/.gradle",
        "!sample/android/build",
        "!sample/android/app/build"
      ],
      "outputs": []
    },
    "build:ios": {
      "cache": false,
      "inputs": [
        "sample/package.json",
        "sample/ios",
        "sample/src/**/*",
        "!sample/ios/build",
        "!sample/ios/Pods"
      ],
      "outputs": ["sample/ios/build"]
    }
  }
}
